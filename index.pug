extends layout.pug

block title
    title Inf

block content
    +header('/groupoid.svg', 'Groupoid Infinity', '<b>Inf</b> &mdash; Language with Dependent Types on top of Trusted &lambda;&nbsp;Core with Changeable Encodings<br>and Extraction to Untyped Lambda Interpreter')
    article.main
        .index
            .index__col
                h2 Specification
                ol
                    li: a(href='/inf') inf language
                    li: a(href='/types') base library
                    li: a(href='/om') trusted om core
                    li: a(href='/extract') extract targets
            .index__col
                h2 Articles
                ul
                    li 15 FEB 16 #[a(href='/intro') intro]
                    li 1 MAY 16 #[a(href='/enc')  lambek encoding ]
                    li 4 AUG 16 #[a(href='/semantics')  type judgment rules ]
                    li 18 MAR 16 #[a(href='/status')  project status ]
            .index__col
                h2 Papers
                ul
                    li 13 AUG 16 #[a(href='http://github.com/groupoid/exe/blob/master/doc/identity.pdf') identities ]
                    li 4 SEP 16 #[a(href='http://github.com/groupoid/exe/blob/master/doc/encoding.pdf') encoding ]
